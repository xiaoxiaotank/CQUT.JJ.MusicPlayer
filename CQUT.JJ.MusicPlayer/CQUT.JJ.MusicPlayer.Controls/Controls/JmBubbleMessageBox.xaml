<Window x:Class="CQUT.JJ.MusicPlayer.Controls.Controls.JmBubbleMessageBox"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CQUT.JJ.MusicPlayer.Controls.Controls"
        mc:Ignorable="d"
        Title="JmBubbleMessageBox" Height="100" Width="200" WindowStyle="None"
        AllowsTransparency="True" WindowStartupLocation="CenterOwner">
    <Grid x:Name="Gd">
        <TextBlock x:Name="TbMsg" Foreground="White" VerticalAlignment="Center" HorizontalAlignment="Left"
                   Margin="5" FontSize="14" TextWrapping="Wrap"/>
    </Grid>
</Window>
